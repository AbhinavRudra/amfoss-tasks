{
  "name": "WeatherCast",
  "description": "a simple chrome extension which displays live weather.",
  "version": "0.0.1",
  "manifest_version": 3,
  "action": {
      "default_popup":"popup.html",
      "default_icon": "images/logo.png"
  },
  "icons":{
    "16": "images/logo.png",
    "48": "images/logo.png",
    "128": "images/logo.png"
  },
  "permissions": ["activeTab", "storage"]

}